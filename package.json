{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install-all": "yarn && cd smart-contracts && yarn",
    "dev": "concurrently \"next dev\" \"cd smart-contracts && yarn watch compilation\"",
    "dev:local-node": "concurrently \"cd smart-contracts && yarn hardhat node\" \"cd smart-contracts && yarn setup-local\"",
    "build": "echo $PUBSUB_KEYFILE > pubsub-keyfile.json && yarn compile && next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "cd smart-contracts && yarn compile",
    "test-contract": "cd smart-contracts && yarn test"
  },
  "dependencies": {
    "date-fns": "^2.24.0",
    "ethers": "^5.4.6",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.20",
    "@types/tailwindcss": "^2.2.1",
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "postcss": "^8.3.6",
    "sass": "^1.42.0",
    "tailwindcss": "^2.2.15",
    "typescript": "4.4.2"
  }
}
