{
  "name": "web-client",
  "version": "0.1.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "install-all": "yarn && cd smart-contracts && yarn",
    "codegen": "graphql-codegen",
    "dev": "yarn codegen && concurrently \"next dev\" \"cd smart-contracts && yarn watch compilation\"",
    "dev:local-node": "yarn codegen && concurrently \"cd smart-contracts && yarn hardhat node\" \"cd smart-contracts && yarn setup-local\"",
    "build": "yarn compile && yarn codegen && next build",
    "start": "next start",
    "check-types": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier -w \"./src/**/*.(js|jsx|ts|tsx|json)\"",
    "compile": "cd smart-contracts && yarn compile",
    "test-contract": "cd smart-contracts && yarn test"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@google-cloud/storage": "^5.18.1",
    "date-fns": "^2.24.0",
    "ethers": "^5.4.6",
    "fabric-pure-browser": "^4.6.0",
    "framer-motion": "5",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^18.1.1",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.5.3",
    "react-fullstory": "^1.4.0",
    "react-scroll": "^1.8.4",
    "react-select": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.2",
    "@neupauer/tailwindcss-plugin-colors": "^1.0.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.0",
    "@types/fabric": "^4.5.3",
    "@types/react": "^17.0.34",
    "@types/react-scroll": "^1.8.3",
    "@types/tailwindcss": "^2.2.1",
    "@types/uuid": "^8.3.1",
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "graphql": "^16.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.0",
    "postcss": "^8.3.6",
    "prettier": "^2.4.1",
    "sass": "^1.42.0",
    "tailwindcss": "^2.2.15",
    "typescript": "4.4.2"
  }
}
